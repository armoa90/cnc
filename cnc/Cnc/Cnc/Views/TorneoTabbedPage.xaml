            xmlns:tabs="clr-namespace:Cnc.Views"
             x:Class="Cnc.Views.TorneoTabbedPage"
            BindingContext="{Binding Main ,Source={StaticResource Locator}}" 
            Title = "{Binding Torneo.Torneo.Descripcion}" >
